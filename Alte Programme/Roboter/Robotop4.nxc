           else if(drz == 1 || drz == 2)
           {
            while(schwarz)
            {
             OnFwd(OUT_AB, motpow);
             while(touch1 || touch2)
             {
              OnFwd(OUT_A, motpow);
              OnRev(OUT_B, motpow);
              Wait(100);
              OnRev(OUT_AB, motpow);
              Wait(10);
             }
            }
            drz++;
            goto dr;
           }
          }
          OnRev(OUT_A, motpow);
          OnRev(OUT_B, motpow-20);
          Wait(3000);
          OnFwd(OUT_A, motpow);
          OnRev(OUT_B, motpow);
          Wait(1000);
          OnRev(OUT_AB, motpow);
          Wait(4000);
          Off(OUT_AB);
          OnFwd(OUT_AB, motpow);
          Wait(3000);
          Off(OUT_ABC);

          PlayTone(659,1600);
          Wait(280);
          PlayTone(622,1600);
          Wait(280);
          PlayTone(659,1600);
          Wait(280);
          PlayTone(622,1600);
          Wait(280);
          PlayTone(659,1600);
          Wait(280);
          PlayTone(494,1600);
          Wait(280);
          PlayTone(587,1600);
          Wait(280);
          PlayTone(523,1600);
          Wait(280);
          PlayTone(440,1600);
          Wait(280);
          PlayFileEx("hi.rso",4,false);
          Off(OUT_AB);
          Wait(1000);

          //OnFwd(OUT_C, 100);
          PlayTone(523,1200);
          OnFwd(OUT_A, motpow);
          OnRev(OUT_B, motpow);
          Wait(3000);
          Off(OUT_ABC);
          d = 0;

          while(true)
          {
           OnFwd(OUT_AB, motpow);

           //-------------------------------------------------------------------Wand
           if(touch1)
           {
            OnFwd(OUT_AB, motpow);
            Wait(200);
            if(dose)goto doser;
            OnRev(OUT_AB, motpow);
            Wait(700);
            OnFwd(OUT_A, motpow);
            OnRev(OUT_B, motpow);
            Wait(1500);
            Timer2 = 0;
           }
           if(touch2)
           {
            OnFwd(OUT_AB, motpow);
            Wait(200);
            if(dose)goto doser;
            OnRev(OUT_AB, motpow);
            Wait(700);
            OnFwd(OUT_A, motpow);
            OnRev(OUT_B, motpow);
            Wait(1500);
            Timer2 = 0;
           }
           //-------------------------------------------------------------------schwarz
           if(schwarz)
           {
            OnRev(OUT_AB, motpow);
            Wait(700);
            OnFwd(OUT_A, motpow);
            OnRev(OUT_B, motpow);
            Wait(1500);
           }
           //-------------------------------------------------------------------silber
           if(silber)
           {
            OnRev(OUT_AB, motpow);
            Wait(700);
            OnFwd(OUT_A, motpow);
            OnRev(OUT_B, motpow);
            Wait(2000);
           }
           if(dose)
            goto doser;
          }
         }
        }
       }
      }
