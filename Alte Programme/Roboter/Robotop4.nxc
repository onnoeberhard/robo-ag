task lcd()
{
  ClearScreen();
  while(true)
  {
   NumOut(1, LCD_LINE1, SENSOR_1);
   NumOut(1, LCD_LINE2, SENSOR_2);
   NumOut(1, LCD_LINE3, SENSOR_3);
   NumOut(1, LCD_LINE4, SENSOR_4);
  }
}

